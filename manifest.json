{
  "manifest_version": 3,
  "name": "Night Light",
  "description": "Adds a dimming overlay to webpages.",
  "version": "1.0",
  "permissions": ["storage", "activeTab"],
  "icons": {
    "16": "data:image/png;base64,",
    "32": "data:image/png;base64,",
    "48": "data:image/png;base64,",
    "128": "data:image/png;base64,"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "data:image/png;base64,",
      "32": "data:image/png;base64,",
      "48": "data:image/png;base64,"
    }
  },
  "options_page": "options.html",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ]
}
